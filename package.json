{
  "name": "vega-lite-dev-config",
  "version": "0.18.0",
  "description": "Version-controlled build config for easy re-use and sharing",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vega/vega-lite-dev-config.git"
  },
  "author": "Kanit \"Ham\" Wongsuphasawat, Dominik Moritz",
  "collaborators": [
    "Kanit Wongsuphasawat <kanitw@gmail.com> (http://kanitw.yellowpigz.com)",
    "Dominik Moritz <domoritz@cs.washington.edu> (https://www.domoritz.de)"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/vega/vega-lite-dev-config/issues"
  },
  "homepage": "https://github.com/vega/vega-lite-dev-config#readme",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "configs",
    "templates",
    "index.d.ts"
  ],
  "bin": {
    "beemo": "./bin/beemo.js"
  },
  "dependencies": {
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@beemo/cli": "2.0.5",
    "@beemo/core": "2.1.3",
    "@beemo/driver-babel": "2.0.5",
    "@beemo/driver-eslint": "2.0.4",
    "@beemo/driver-jest": "2.0.4",
    "@beemo/driver-prettier": "2.0.5",
    "@beemo/driver-typescript": "2.1.1",
    "@types/eslint": "^8.2.1",
    "@types/jest": "^27.4.0",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "babel-jest": "^27.4.5",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^26.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "jest": "^27.4.5",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4"
  },
  "devDependencies": {
    "@auto-it/conventional-commits": "^10.32.5",
    "@auto-it/first-time-contributor": "^10.32.5",
    "@types/node": "^17.0.7",
    "auto": "^10.32.5"
  },
  "scripts": {
    "prepare": "beemo create-config",
    "lint": "beemo prettier --check './configs/*.ts'",
    "format": "beemo prettier --write './configs/*.ts'",
    "release": "yarn prepare && auto shipit"
  }
}
