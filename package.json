{
  "name": "vega-lite-dev-config",
  "version": "0.15.1",
  "description": "Version-controlled build config for easy re-use and sharing",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vega/vega-lite-dev-config.git"
  },
  "author": "Kanit \"Ham\" Wongsuphasawat, Dominik Moritz",
  "collaborators": [
    "Kanit Wongsuphasawat <kanitw@gmail.com> (http://kanitw.yellowpigz.com)",
    "Dominik Moritz <domoritz@cs.washington.edu> (https://www.domoritz.de)"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/vega/vega-lite-dev-config/issues"
  },
  "homepage": "https://github.com/vega/vega-lite-dev-config#readme",
  "files": [
    "index.js",
    "lib",
    "templates"
  ],
  "dependencies": {
    "@babel/core": "^7.12.17",
    "@babel/preset-env": "^7.12.17",
    "@babel/preset-typescript": "^7.12.17",
    "@beemo/cli": "^1.0.11",
    "@beemo/core": "^1.1.8",
    "@beemo/driver-babel": "^1.2.6",
    "@beemo/driver-eslint": "^1.2.3",
    "@beemo/driver-jest": "^1.2.3",
    "@beemo/driver-prettier": "^1.3.2",
    "@beemo/driver-typescript": "^1.4.1",
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "babel-jest": "^26.6.3",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-jest": "^24.1.5",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1"
  },
  "devDependencies": {
    "@types/node": "^14.14.31",
    "typescript": "~4.1.5"
  },
  "peerDependencies": {
    "typescript": "~4.1.5"
  },
  "beemo": {
    "module": "@local",
    "drivers": [
      "prettier"
    ]
  },
  "scripts": {
    "build": "tsc",
    "prepare": "tsc && beemo create-config",
    "lint": "beemo prettier --check \"./src/**/*.ts\"",
    "format": "beemo prettier --write \"./src/**/*.ts\""
  }
}
